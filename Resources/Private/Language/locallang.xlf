<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.0" xmlns:t3="http://typo3.org/schemas/xliff">
	<file t3:id="1568292127" source-language="en" datatype="plaintext" original="EXT:redirects/Resources/Private/Language/locallang.xlf" date="2017-12-29T20:22:14Z" product-name="redirects">
		<header/>
		<body>
			<trans-unit id="redirects" resname="redirects">
				<source>Redirect Settings</source>
			</trans-unit>
			<trans-unit id="redirects.autoUpdateSlugs" resname="redirects.autoUpdateSlugs">
				<source>Update the slugs of all sub pages automatically</source>
			</trans-unit>
			<trans-unit id="redirects.autoCreateRedirects" resname="redirects.autoCreateRedirects">
				<source>Create redirects for pages with a new slug automatically</source>
			</trans-unit>
			<trans-unit id="redirects.redirectTTL" resname="redirects.redirectTTL">
				<source>Time To Live of redirect records in days</source>
			</trans-unit>
			<trans-unit id="redirects.httpStatusCode" resname="redirects.httpStatusCode">
				<source>HTTP Status Code for the redirect, 301 is the default</source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.name" resname="cleanupRedirectsTask.name">
				<source>Cleanup Redirects Task</source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.description" resname="cleanupRedirectsTask.description">
				<source>Cleanup old redirects periodically for defined conditions like age, hit count or domains.</source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.label.days" resname="cleanupRedirectsTask.label.days">
				<source><![CDATA[Age of records in days ( query usage: createdon < :age )]]></source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.label.domains" resname="cleanupRedirectsTask.label.domains">
				<source><![CDATA[Domain(s) comma separated ( query usage: source_host IN (:domains) )]]></source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.label.hitCount" resname="cleanupRedirectsTask.label.hitCount">
				<source><![CDATA[Hit Count ( query usage: hitcount < :hitCount )]]></source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.label.path" resname="cleanupRedirectsTask.label.path">
				<source><![CDATA[Path pattern ( query usage: source_path LIKE :path )]]></source>
			</trans-unit>
			<trans-unit id="cleanupRedirectsTask.label.statusCodes" resname="cleanupRedirectsTask.label.statusCodes">
				<source><![CDATA[Status code(s) comma separated ( query usage: target_statuscode IN (:statusCodes) )]]></source>
			</trans-unit>
		</body>
	</file>
</xliff>
